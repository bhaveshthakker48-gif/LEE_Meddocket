<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Base.Theme.LLE_MedDocket" parent="Theme.Material3.DayNight.NoActionBar">
        <item name="colorPrimary">@color/blue</item>
        <!-- Customize your light theme here. -->
        <!-- <item name="colorPrimary">@color/my_light_primary</item> -->
    </style>
    <style name="AppTheme" parent="Theme.AppCompat.DayNight.NoActionBar">
        <item name="colorPrimary">@color/blue</item>
        <item name="colorPrimaryDark">@color/blue</item>
        <item name="colorAccent">@color/blue</item>
        <!-- <item name="actionBarTheme">@style/toolbar_style</item>-->
    </style>

    <style name="Theme.LLE_MedDocket" parent="Base.Theme.LLE_MedDocket" />

    <style name="loginStyle" parent="Widget.MaterialComponents.TextInputLayout.OutlinedBox.Dense">
        <item name="materialThemeOverlay">
            @style/ThemeOverlay.MaterialComponents.TextInputEditText.OutlinedBox
        </item>
        <item name="shapeAppearance">@style/ShapeAppearance.Material3.SmallComponent</item>
        <item name="android:textColorHint">@color/white</item>
        <item name="boxStrokeColor">@color/white</item>
        <item name="android:fontFamily">@font/opensans_regular</item>
        <item name="boxStrokeWidth">1.5dp</item>
        <item name="cornerFamily">cut</item>
        <item name="cornerSize">20sp</item>
        <item name="boxStrokeErrorColor">@color/red</item>
        <item name="hintTextColor">@color/white</item>
        <item name="boxCollapsedPaddingTop">0dp</item>
    </style>


    <style name="MyCustomToolbarTextAppearance" parent="TextAppearance.Design.CollapsingToolbar.Expanded">
        <item name="android:textAllCaps">false</item>
        <item name="android:textSize">18sp</item>

    </style>

    <style name="edit_text_style" parent="@android:style/Widget.Material.EditText">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:textSize">15sp</item>
        <item name="android:hint">Enter</item>
        <item name="android:padding">10dp</item>
        <item name="fontFamily">@font/opensans_semibold</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="edit_text_style_new" parent="@android:style/Widget.Material.EditText">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:textSize">15sp</item>
        <item name="android:padding">10dp</item>
        <item name="fontFamily">@font/opensans_semibold</item>
        <item name="android:textColor">@color/black</item>
    </style>
